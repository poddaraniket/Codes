#include <bits/stdc++.h>

using namespace std;

int main() {
  vector <double> v;
  int n,i;
  double num;
  int s=0;
  cin>>n;   
  for(i=0;i<n;i++)
    {
      cin>>num;
      v.push_back(num);
      //cout<<trunc(num)<<endl;
      s+=trunc(num); 
    }
  cout<<s<<endl;  
  if(s<0)
   {
     int p=abs(s);
     while(p)
     {
     for(i=0;i<v.size();i++)
       {
        if(v[i]>0)
         {
           if(ceil(v[i])-floor(v[i])!=0)
            {
               v[i]=ceil(v[i]);
               p--;
            }
         }
       }
     }
   }
  else if(s>0)
    {
       while(s)
       {
        for(i=0;i<v.size();i++)
        {
         if(v[i]<0)
          {
           if(ceil(v[i])-floor(v[i])!=0)
            {
               v[i]=floor(v[i]);
               s--;
            }
          }
        }
       }
    }
 for(i=0;i<v.size();i++)
    {
      cout<<trunc(v[i])<<endl;
    }     
}
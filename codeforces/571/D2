#include <bits/stdc++.h>

using namespace std;

int main() {
 vector<double> v,v1;
 int i,n,s=0,j=0;
 double num;
 cin>>n;
 for(i=0;i<n;i++)
  {
    cin>>num;
    v.push_back(num);
    v1.push_back(floor(num));
    s+=v1[i];
  }
  int p=abs(s);
  //cout<<p<<endl;
 // cout
  while(p)
   {
     if(v[j]-v1[j]!=0)
     {
       v1[j]+=1;
       j++;
       p--;
     }
     else
      j++;
   }
  for(i=0;i<v.size();i++)
   {
     cout<<v1[i]<<endl;
   } 
}
#include <bits/stdc++.h>

using namespace std;

void prime(int n)
{
    int i,p=0;
    for(i=2;i*i<=n;i++)
     {
         if(n%i==0)
          {
            p=1;
            cout<<"NO"<<endl;
            break;
          }
     }
    if(p==0)
     cout<<"YES"<<endl;
}

int main()
{
    long long int n,i,j,c=0,p=0;
    cin>>n;
    long long int a[n];
    double b[n];
    for(i=0;i<n;i++)
     {
         cin>>a[i];
     }
    for(i=0;i<n;i++)
     {
        b[i]=sqrt(a[i]);
     }
    for(i=0;i<n;i++)
      {
          if(ceil(b[i])-b[i]!=0)
            cout<<"NO"<<endl;
          else
           {
               if(a[i]==1 || a[i]==2 || a[i]==3)
                 cout<<"NO"<<endl;
               else
                 prime(b[i]);
           }
      }
}
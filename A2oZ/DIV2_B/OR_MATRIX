#include <bits/stdc++.h>

using namespace std;

int main() {
  int m,n,i,j,d=0;
  cin>>m>>n;
  int b[m+1][n+1];
  int a[m+1][n+1];
  //int c[m][n];
  int ro[100]={0};
  int co[100]={0};
  int ver[100]={0};
  int hor[100]={0};
  for(i=1;i<=m;i++)
   {
     for(j=1;j<=n;j++)
      {
        cin>>b[i][j];
        if(b[i][j]==0)
         {
            ro[i]++;
            co[j]++;
         }
      }
   }
  for(i=1;i<=m;i++)
   {
     for(j=1;j<=n;j++)
      {
        if(ro[i]>0)
          a[i][j]=0;
        else if(co[j]>0)
          a[i][j]=0;
        else
          a[i][j]=1;  
      }
   }
  for(i=1;i<=m;i++)
   {
     for(j=1;j<=n;j++)
      {
        if(a[i][j]==1)
         {
            ver[i]=1;
            hor[j]=1;
         }
      }
   }
  for(i=1;i<=m;i++)
   {
     for(j=1;j<=n;j++)
       {
         if(b[i][j]!= (ver[i]|hor[j]))
           {
             d=1;
             break;
           }
       }
   }
  if(d==1) 
    cout<<"NO"<<endl;
  else
    {
      cout<<"YES"<<endl;
      for(i=1;i<=m;i++)
        {
           for(j=1;j<=n;j++)
            {
            cout<<a[i][j]<<" ";
            }
          cout<<endl; 
        }  
    }
}
#include <bits/stdc++.h>

using namespace std;

int main() {
  int k=0,i;
  string a;
  int s[27]={0};
  cin>>a;
  for(i=0;i<a.length();i++)
   {
     s[a[i]-97]++;
   }
  for(i=0;i<27;i++)
   {
     if(s[i]%2!=0)
      k++;
   } 
  if(k>1)
   {
     if(k%2==0)
      cout<<"Second"<<endl;
     else
       cout<<"First"<<endl; 
   } 
  else
    cout<<"First"<<endl; 
}
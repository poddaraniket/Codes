#include<iostream>
 
using namespace std;
 
int a[4][4],S,s1,s2,s3;
 
int main()
{
  for(int i=0;i<3;i++)
    for(int j=0;j<3;j++)
      cin>>a[i][j];
  s1=a[0][0]+a[1][0]+a[2][0];
  s2=a[0][1]+a[1][1]+a[2][1];
  s3=a[0][2]+a[1][2]+a[2][2];
  S=max(max(s1,s2),s3);
  a[0][0]=S-s1;
  a[1][1]=S-s2;
  a[2][2]=S-s3;
  while(a[0][0]+a[2][2]!=a[0][2]+a[2][0])
    a[0][0]++,a[1][1]++,a[2][2]++;
  for(int i=0;i<3;cout<<endl,i++)
    for(int j=0;j<3;j++)
      cout<<a[i][j]<<" ";
  return 0;
} 
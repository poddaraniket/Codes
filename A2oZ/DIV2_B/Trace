#include <bits/stdc++.h>
 
using namespace std;
 
int main() {
   int n,s=0,i;
   cin>>n;
   int a[n];
   for(i=0;i<n;i++)
    {
      cin>>a[i];
    }
   sort(a,a+n);
   for(i=n-1;i>=0;i-=2)
    {
       s+=(a[i]*a[i]);
       if(i-1>=0)
        s-=(a[i-1]*a[i-1]); 
    }
   cout<<fixed << setprecision(10)<<s*3.1415926536<<endl;
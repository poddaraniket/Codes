#include <bits/stdc++.h>
using namespace std;

int BC(int n,int k)
{
    int i,j;
    int C[n+1][k+1]={0};
    for(i=0;i<=n;i++)   // Binomial Coefficient  ---> similar to fibonacci
     {
         for(j=0;j<=min(i,k);j++)
          {
              if(j==0 || j==i)
                C[i][j]=1;
              else
                C[i][j]=C[i-1][j-1]+C[i-1][j];
          }
     }
    return C[n][k]; 
}

int main()
{
    int n,k;
    cin>>n>>k;
    cout<<BC(n,k);
    return 0;  
}
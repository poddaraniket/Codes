#include <iostream>
using namespace std;

int eggDrop(int n,int k)
{
    int eggs[n+1][k+1];
    int res,i,j,x;
    for(i=1;i<=n;i++)
      {
          eggs[i][1]=1;
          eggs[i][0]=0;
      }
    for(j=1;j<=k;j++)
       eggs[1][j]=j;
    for(i=2;i<=n;i++)
     {
       for(j=2;j<=k;j++)
        {
            eggs[i][j]=10000;
            for(x=1;x<=j;x++)
              {
                  res=1+max(eggs[i-1][x-1],eggs[i][j-x]);
                  if(res<eggs[i][j])
                    eggs[i][j]=res;
              }              
        }
     }
    return eggs[n][k]; //
}

int main() {
	// your code goes here
    int n = 2, k = 36; 
    cout << "Minimum number of trials in worst case with "
         << n << " eggs and " << k << " floors is " 
         << eggDrop(n, k) << endl; 
	return 0;
}

#include <bits/stdc++.h>
using namespace std;

int lcs(char *x,char *y,int m,int n)
{
    string s="";
    int l[m+1][n+1];  
    int i,j;
    for(i=0;i<=m;i++)   // generating the Length longest common subsequence
      {
          for(j=0;j<=n;j++)
            {
                if(i==0 || j==0)
                    l[i][j]=0;
                else
                  {
                      if(x[i-1]==y[j-1])
                        l[i][j]=1+l[i-1][j-1];
                      else
                        l[i][j]=max(l[i-1][j],l[i][j-1]);
                  }
            }
      }
    i=m;
    j=n;
    while(i && j)       // getting the longest common subsequence
     {
        if(l[i][j]>l[i-1][j] && l[i][j]>l[i][j-1])
         {
             s=x[i-1]+s;
             i--;
             j--;
         }
        else{
                if(l[i][j]==l[i-1][j])
                  i--;
                else
                  j--;
            } 
     }
    cout<<s<<endl; 
    return l[m][n];  
}

int main()
{
    char X[] = "AGGTAB";  
    char Y[] = "GXTXAYB";  
      
    int m = strlen(X);  
    int n = strlen(Y);  
      
    cout<<"Length of LCS is "<< lcs( X, Y, m, n ) ;  
      
    return 0;  
}